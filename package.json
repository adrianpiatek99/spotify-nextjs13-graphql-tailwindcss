{
  "name": "spotify-nextjs-graphql",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint-ts": "eslint *.ts **/*.ts **/*.tsx",
    "lint-fix": "eslint --fix *.ts **/*.ts **/*.tsx",
    "lint": "yarn run lint-ts",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit",
    "generate-icons": "rimraf \"./icons\" && svgr --config-file svgr-icons.config.js -d src/icons assets/icons && eslint --fix src/icons && prettier --write src/icons",
    "generate-gql": "graphql-codegen && eslint --fix src/graphql/graphql.ts && prettier --write src/graphql/graphql.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.5.0",
    "@as-integrations/next": "^1.2.0",
    "@headlessui/react": "^1.7.13",
    "@prisma/client": "^4.11.0",
    "@tanstack/react-query": "^4.27.0",
    "class-validator": "^0.14.0",
    "graphql": "^16.6.0",
    "graphql-request": "5.1.0",
    "next": "13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "tailwind-merge": "^1.10.0",
    "type-graphql": "2.0.0-beta.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@svgr/cli": "^6.5.1",
    "@tanstack/eslint-plugin-query": "^4.27.0",
    "@types/node": "^18.15.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-type-graphql": "^1.0.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "rimraf": "^4.4.0",
    "tailwindcss": "^3.2.7",
    "typescript": "^5.0.2"
  }
}
